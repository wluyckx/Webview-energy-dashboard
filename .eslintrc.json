{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true,
    "commonjs": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "script"
  },
  "globals": {
    "Config": "readonly",
    "PowerFlow": "readonly",
    "App": "readonly",
    "ApiClient": "readonly",
    "MockData": "readonly",
    "KpiStrip": "readonly",
    "EnergyBalance": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
